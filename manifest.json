{
  "manifest_version": 3,
  "name": "Imagine Job Email Insert",
  "version": "1.0",
  "description": "Imagine Job Email Insert",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/forms/*"],
      "js": ["app.js"]
    }
  ]
}